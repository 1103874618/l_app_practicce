.warp{
  height: 100%;
  display: flex;
  flex-direction:  column;
  padding: 20rpx;
}
navigator{
  overflow: hidden;
}

.list{
  margin-bottom: 20rpx;
  height: 200rpx;
  position: relative;
}
.imgs{
  display: block;
  float: left;
}
.imgs image{
  display: block;
  width: 200rpx;
  height: 200rpx;
}
.infos{
  float: left;
  width: 480rpx;
  height: 200rpx;
  padding: 20rpx 0 0 20rpx;
  
}
.date{
  font-size: 18rpx;
  color: #aaa;
  position: absolute;
  bottom: 0;
}
.title{
  font-size: 40rpx;
}
.botton_1{
  margin: 1rem 0;
}
button{
  border-radius: 10px;
} 
.view1{
  height: 500rpx;
  width: 100%;
  background: purple;
}
.view2{
  height: 400rpx;
  width: 80%;
  background: yellow;
}
.view3{
  height: 300rpx;
  width: 60%;
  background: red;
}
.li_block{
  margin:2rem 0;
  display: block;
  width: 100%;
  height: 200rpx;

}
